public class Oppgave7 extends EasyGraphics {
	
	 public static void main(String[] args) {
		    launch(args);
		  }

		  public void run() {
			  
			  
			 
			  
			  makeWindow("Statistikker for antall gutter og jenter");
			  
			  
			  // A-Klassen
			  setColor(0, 250, 0);
			    fillRectangle(20, 30, 100, 40);
			    setColor(255, 0, 0);
			    fillRectangle(100, 30 , 180, 40);
			   setColor(0, 0, 0);
			    drawRectangle(20, 30, 250, 40);
			   // B-Klassen
			    /*setColor(0, 250, 0);
