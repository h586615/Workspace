package no.hvl.dat100;

import easygraphics.EasyGraphics;
import static javax.swing.JOptionPane.*;
import static java.lang.Double.*;

public class Oppgave7 extends EasyGraphics {
	
	 public static void main(String[] args) {
		    launch(args);
		  }

		  public void run() {
			  
			  setColor(0, 250, 0);
			    fillRectangle(20, 50, 48, hoyde);
			    setColor(255, 0, 0);
			    fillRectangle(50, 50 , lengde, hoyde);
			    setColor(250, 250, 0);
			    fillRectangle(50, 50 + 2*hoyde, lengde, hoyde);
			    setColor(0, 0, 0);
	
	
          }
}